{
  "name": "@gp/api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "repository": "https://github.com/loicpennequin/hero-clash.git",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "predev": "yarn run db:start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:create": "docker run --name game-portal -e POSTGRES_PASSWORD=root -e POSTGRES_USER=root -e POSTGRES_DB=game-portal -p 5432:5432 -d postgres:alpine -c 'listen_addresses=*'",
    "db:destroy": "docker container rm game-portal",
    "db:start": "docker start game-portal",
    "db:stop": "docker stop game-portal",
    "typeorm": "node --require ts-node/register -r tsconfig-paths/register  ../../node_modules/typeorm/cli.js --config scripts/database/ormconfig.ts",
    "fixtures": "node --require ts-node/register -r tsconfig-paths/register ../../node_modules/typeorm-fixtures-cli/dist/cli.js ./scripts/database/fixtures --config scripts/database/fixtures/ormconfig.ts --debug"
  },
  "dependencies": {
    "@golevelup/ts-jest": "^0.3.1",
    "@gp/shared": "*",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/event-emitter": "^0.0.3",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.14",
    "@nestjs/platform-socket.io": "^7.6.14",
    "@nestjs/swagger": "^4.7.16",
    "@nestjs/throttler": "^1.1.1",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.14",
    "@types/lodash": "^4.14.171",
    "@types/lodash-es": "^4.17.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "apollo-server": "^2.25.2",
    "apollo-server-express": "^2.25.0",
    "apollo-server-plugin-base": "^0.13.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.22.1",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jest-mock-req-res": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.25",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "socket.io": "^4.0.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.2.1",
    "typeorm": "^0.2.37",
    "typeorm-fixtures-cli": "^1.9.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "25.2.3",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.35",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "eslint": "7.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "26.0.1",
    "jest-extended": "^0.11.5",
    "jest-mock-req-res": "^1.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.5.6",
    "ts-loader": "^6.2.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
